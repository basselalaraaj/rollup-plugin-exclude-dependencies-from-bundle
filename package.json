    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "8.22.0",
    "@typescript-eslint/parser": "8.22.0",
    "eslint": "9.19.0",
    "jest": "29.7.0",
    "lint-staged": "15.4.3",
    "prettier": "3.4.2",
    "rimraf": "^6.0.1",
    "rollup": "4.32.1",
